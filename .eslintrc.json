{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true,
		"jquery": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended",
		"plugin:prettier/recommended"
	],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "warn",
		"indent": ["error", "tab"],
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"linebreak-style": ["error", "unix"],
		"no-unused-vars": ["warn"],
		"no-console": "off",
		"@wordpress/no-wp-process-env": "off"
	}
}
